fof(p1,conjecture,(! [T,C1,C2,C3] : 
(
%  (T > 0 &
%   (C1 + 0.1)^2/0.0064 + (C2 - 0.85)^2/0.0049 - (C3 - 0.9)^2/0.04 < 1)
  (T > 0 &	 
   C1 > -0.1 & C1 < 0.1 &
   C2 > 0.8 & C2 < 1 &
   C3 > 0.9 & C3 < 1)
   => 
   ~(
	((C1*cos(T) - C2*sin(T))*exp(-0.1*T))^2/0.02 +
	((C2*cos(T) + C1*sin(T))*exp(-0.1*T))^2/0.02 +
   	 ((C3*exp(-0.15*T) - 0.5)^2)/0.02 < 1)    	      
   ))).


