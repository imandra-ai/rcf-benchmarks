fof(p1,conjecture,(! [T,C1,C2,C3,S,C] : 
(
  (T > 20 &
   C1 > -0.1 & C1 < 0.1 &
   C2 > 0.8 & C2 < 1 &
   C3 > 0.9 & C3 < 1 &
   S^2+C^2=1) =>
   (
	((C1*C - C2*S)*exp(-0.1*T)) < 0.2 &
	((C1*C - C2*S)*exp(-0.1*T)) > -0.2 &
    	((C2*C + C1*S)*exp(-0.1*T)) < 0.2 &
	((C2*C + C1*S)*exp(-0.1*T)) > -0.2 &
   	 (C3*exp(-0.15*T) > -0.2) &
	 (C3*exp(-0.15*T) < 0.2))      	      
   ))).


